<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="KinEcosystemDashView">
        <attr format="color" name="kinEcosystemColor"/>
        <attr format="dimension" name="kinEcosystemDashLength"/>
        <attr format="dimension" name="kinEcosystemDashGap"/>
        <attr format="dimension" name="kinEcosystemDashThickness"/>
        <attr format="enum" name="kinEcosystemOrientation">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="KinEcosystemBalanceView">
        <attr format="string" name="kinEcosystemSubTitle"/>
    </declare-styleable>

    <declare-styleable name="KinEcosystemTouchIndicatorIcon">
        <attr format="reference" name="src"/>
        <attr name="kinecosystem_indicatorVisibility"/>
    </declare-styleable>

    <declare-styleable name="KinEcosystemSettingsItem">
        <attr format="reference" name="kinecosystem_src"/>
        <attr format="string" name="kinecosysem_text"/>
        <attr name="kinecosystem_indicatorVisibility"/>
    </declare-styleable>

    <attr format="boolean" name="kinecosystem_indicatorVisibility"/>
</resources>